{
  "name": "nextjs-starter-tailwind",
  "version": "0.1.0",
  "author": "Maurits Misana <maurits@techmaus.nl>",
  "license": "Unlicense",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "eslint-check": "eslint --print-config /pages/index.jsx | eslint-config-prettier-check",
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '{pages,components,css}/**/*.{js,jsx,css,md,json}'",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '{pages,components,css}/**/*.{js,jsx,css,md,json}'",
    "fix": "npm run fix:lint && npm run fix:prettier"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@tailwindcss/ui": "^0.1.4",
    "autoprefixer": "^9.7.6",
    "cssnano": "^4.1.10",
    "next": "9.3.5",
    "postcss-import": "^12.0.1",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "tailwindcss": "^1.3.5"
  },
  "browserslist": [
    ">0.3% in NL",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "prettier": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TECHMAUS/nextjs-starter-tailwind"
  }
}
